{
  "name": "toptal",
  "version": "1.0.0",
  "description": "REST API that shows time in different timezones",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "start": "node api/index.js",
    "dev": "nodemon api/index.js",
    "dev:debug": "nodemon --inspect=0.0.0.0:9220 api/index.js",
    "db:sync": "node database/utils/sync.js",
    "db:sync:force": "node database/utils/sync.js -f",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all",
    "db:seed": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo",
    "db:seed:undo:all": "sequelize db:seed:undo:all",
    "cover": "nyc --report-dir ./coverage yarn run test",
    "test": "node_modules/mocha/bin/_mocha -- -R spec test/index.js --exit --timeout 10000",
    "test:unit": "node_modules/mocha/bin/_mocha -- -R spec test/unit/index.js --exit",
    "test:integration": "node_modules/mocha/bin/_mocha -- -R spec test/integration/index.js --exit --timeout 20000",
    "test:component": "node_modules/mocha/bin/_mocha -- -R spec test/component/index.js --exit"
  },
  "author": "Joel Barenco <joelbarenco@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@casl/ability": "^5.4.3",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "commander": "^8.3.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "highland": "^2.13.5",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "nconf": "^0.11.3",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "ramda": "^0.27.1",
    "request-ip": "^2.1.3",
    "restify-errors": "^8.0.2",
    "sequelize": "^6.8.0",
    "superagent": "^6.1.0",
    "validator": "^13.6.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "chai": "^4.3.4",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.14",
    "nyc": "^15.1.0",
    "rewire": "^5.0.0",
    "sinon": "^11.1.2",
    "supertest": "^6.1.6",
    "uuid": "^8.3.2"
  }
}
